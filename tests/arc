>>> (def stream (xs)
      (fn ()
        (if xs
             (do1 (car xs)
                  (= xs (cdr xs))))))
#<procedure...>

>>> (let s (stream '(a b c))
      (list (s)
            (s)
            (s)
            (s)))
(a b c nil)

>>> (w/msec (stream '(-205086062 -205086061))
      (tostring (time (+ 8 90))))
