> ((fn (a (b) . c) (list a b c)) 1 '(2) 3)
(1 2 (3))


> (def foo (a (o b 5) (o c 10))
    (list a b c))

> (foo 1)
(1 5 10)

> (foo 1 nil nil)
(1 nil nil)

> (foo 1 nil 3)
(1 nil 3)

> (foo 1 2 3)
(1 2 3)


;=============================================================================
;  let
;=============================================================================

> (let (a b) (list 1 2) a)
1

> (let (a b) (list 1 2) b)
2

> (let (a b) (list 1 2) c)
error: reference to undefined identifier: c


> (let (a (o b 3)) (list 1) a)
1

> (let (a (o b 3)) (list 1) b)
3


> (let (a . b) (list 1 2) a)
1

> (let (a . b) (list 1 2) b)
(2)


> (let (a) (list 1 2) a)
1


> (let (a . b) '(1 . 2)
    a)
1

> (let (a . b) '(1 . 2)
    b)
2


> (withs (foo      (list 1 2 3)
          (a . b)  foo)
    (= b.0 4)
    (is foo.1 4))
t


> (let (a (o b 5)) (list 1 2) a)
1

> (let (a (o b 5)) (list 1 2) b)
2

> (let (a (o b 5)) (list 1) b)
5

> (let (a (o b)) (list 1) b)
nil


> (let (a (o b 5) c) (list 1 2) a)
1

> (let (a (o b 5) c) (list 1 2) b)
2

> (let (a (o b 5) c) (list 1 2) c)
nil

> (let (a (o b 5) c) (list 1 2 3) c)
3


